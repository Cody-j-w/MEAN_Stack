<body class="bg-secondary">
    <div class="container">
        <div class="row">
            <div class="jumbotron mx-auto bg-dark col-6">
                <h1 class="display-1 text-info text-center">Hoo</h1>
            </div>
        </div>
        <div class="row">
                <hr class="my-3 bg-info col-6">
                <a href="/new" class="btn btn-info text-dark col-6 mx-auto text-center">Add an owl to the parliment</a>
                <hr class="my-3 bg-info col-6">
        </div>
            <table class="table table-dark table-striped table-bordered">
                <% for(var owl in owls){%>
                <tr>
                <th class="text-info"><a class="text-info display-1" href="/owls/<%=owls[owl]._id%>"> <%=owls[owl].name%></a></th>
                </tr>
                
                <%}%>
            </table>
        
        
    </div>
</body>
</html>